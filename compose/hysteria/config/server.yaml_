##

listen: :3389

# openssl req -x509 -nodes -days 365 -newkey rsa:2048 -keyout 1.key -out 1.crt
tls:
  cert: /var/lib/cert/1.crt
  key: /var/lib/cert/1.key

auth:
  type: password
  password: <password>

bandwidth:
  up: 1024mbps
  down: 1024mbps

resolver:
  tls:
    addr: 1.1.1.1:853
    timeout: 10s
    sni: cloudflare-dns.com
    insecure: false

masquerade:
  type: proxy
  proxy:
    url: https://www.bilibili.com
    rewriteHost: true
