{
  "files.associations": {
    "*.yaml_": "yaml",
    "*.json_": "json",
    "*.yml_": "yaml"
  },
  "editor.fontSize": 13,
  "editor.tabSize": 2,
  "editor.wordWrap": "on",
  "editor.formatOnSave": true,
  "[yaml]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "editor.autoIndent": "keep",
    "diffEditor.ignoreTrimWhitespace": false
  }
}
